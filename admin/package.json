{
  "name": "modern-velzon-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/runtime": "^7.20.13",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "4.11.3",
    "@mui/material": "5.10.1",
    "antd": "^4.14.0",
    "apexcharts": "^3.36.3",
    "axios": "0.26.0",
    "bootstrap": "5.1.3",
    "eslint": "8.9.0",
    "firebase": "^9.6.7",
    "html2pdf.js": "^0.10.1",
    "moment": "^2.29.4",
    "moment-duration-format": "^2.3.2",
    "react": "17.0.2",
    "react-apexcharts": "^1.4.0",
    "react-dom": "17.0.2",
    "react-flatpickr": "^3.10.13",
    "react-i18next": "11.15.3",
    "react-meta-tags": "1.0.1",
    "react-mustache-template-component": "^1.0.1",
    "react-redux": "7.2.6",
    "react-router-dom": "5.2.0",
    "react-scripts": "5.0.0",
    "react-select": "^5.7.0",
    "react-spinners": "^0.13.7",
    "react-table": "7.7.0",
    "react-toastify": "8.1.1",
    "react-transliterate": "^1.1.9",
    "react-tsparticles": "1.40.0",
    "reactstrap": "9.0.1",
    "redux": "4.1.2",
    "redux-saga": "1.1.3",
    "sass": "^1.57.1",
    "simplebar-react": "2.3.6",
    "swiper": "^6.0.4",
    "transliteration": "^2.3.5",
    "web-vitals": "2.1.4",
    "xlsx": "^0.18.0"
  },
  "overrides": {
    "autoprefixer": "10.4.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write \"**/*.{js,jsx,json,md,html,css,less,scss}\""
  },
  "babelOptions": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "react-app"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "requireConfigFile": false,
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "es6": true
    },
    "globals": {
      "$": true
    },
    "settings": {
      "react": {
        "version": "17.0.1"
      }
    },
    "rules": {
      "semi": 0,
      "react/no-unescaped-entities": 0,
      "react/react-in-jsx-scope": "off",
      "react/prop-types": "off",
      "no-unused-vars": "off",
      "no-extra-boolean-cast": 0
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "moment": "2.29.1"
  },
  "devDependencies": {
    "babel-plugin-prismjs": "2.1.0"
  }
}
